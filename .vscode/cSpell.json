{
	"version": "0.2",
	"language": "en",
	"enableGlobDot": true,
	"useGitignore": true,
	"ignorePaths": [
		"node_modules",
		"dist",
		".git"
	],
	"dictionaries": [],
	// Before adding a words, see if there is already a dictionary in which it is contained: `npx cspell trace '{{WORD}}'`
	"words": [
		"npmignore",
		"postpack",
		"preinstall",
		"roydukkey",
		"tsdoc"
	]
}
